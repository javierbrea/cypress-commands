{
  "name": "@mocks-server/cypress-commands",
  "version": "1.0.2",
  "description": "Extends Cypress' cy commands with methods for administrating mocks-server",
  "keywords": [
    "cypress",
    "commands",
    "mocks-server",
    "mock-server",
    "administration",
    "admin",
    "api",
    "rest",
    "client"
  ],
  "author": "Javier Brea",
  "license": "MIT",
  "repository": "https://github.com/mocks-server/cypress-commands",
  "homepage": "https://www.mocks-server.org",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "index.js",
    "src"
  ],
  "main": "index.js",
  "scripts": {
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "eslint src test index.js jest.config.js test-e2e/cypress test-e2e/mocks test-e2e/app-fetch/public test-e2e/app-data-provider/src test-e2e/app-data-provider/webpack.config.js",
    "lint-staged": "lint-staged",
    "test": "jest",
    "test-e2e": "cd test-e2e && npm run test:ci",
    "test-e2e:ci": "cd test-e2e && npm i && npm run apps:install && npm run test:ci"
  },
  "dependencies": {
    "@data-provider/core": "1.8.0",
    "@mocks-server/admin-api-client": "1.0.2"
  },
  "devDependencies": {
    "babel-eslint": "10.0.3",
    "coveralls": "3.0.9",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "4.0.7",
    "jest": "24.9.0",
    "lint-staged": "9.5.0",
    "prettier": "1.19.1",
    "sinon": "8.0.4"
  },
  "peerDependencies": {
    "cypress": "^2.1.0 || ^3.0.0"
  },
  "lint-staged": {
    "test-e2e/app-data-provider/*.js": "eslint",
    "test-e2e/app-data-provider/src/*.js": "eslint",
    "test-e2e/app-fetch/public/*.js": "eslint",
    "test-e2e/mocks/**/*.js": "eslint",
    "test-e2e/cypress/**/*.js": "eslint",
    "jest.config.js": "eslint",
    "index.js": "eslint",
    "src/**/*.js": "eslint",
    "test/**/*.js": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  }
}
